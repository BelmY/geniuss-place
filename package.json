{
  "name": "geniuss-place",
  "version": "1.0.1",
  "description": "GNSS-SDR website npm build scripts",
  "repository": {
    "type": "git",
    "url": "git://github.com/gnss-sdr/geniuss-place.git"
  },
  "keywords": [
    "gnss",
    "sdr",
    "website"
  ],
  "author": "Carles Fernandez-Prades",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/gnss-sdr/geniuss-place/issues"
  },
  "homepage": "https://gnss-sdr.github.io/geniuss-place/",
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "onchange": "^5.1.3",
    "uglify-js": "^3.4.9"
  },
  "scripts": {
    "uglify": "uglifyjs assets/js/vendor/jquery/jquery-3.3.1.min.js assets/js/plugins/jquery.fitvids.js assets/js/plugins/jquery.greedy-navigation.js assets/js/plugins/jquery.magnific-popup.js assets/js/plugins/jquery.smooth-scroll.min.js assets/js/_main.js -c -m -o assets/js/main.min.js",
    "watch:js": "onchange \"assets/js/**/*.js\" -e \"assets/js/main.min.js\" -- npm run build:js",
    "build:js": "npm run uglify"
  }
}
